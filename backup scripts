<script type="text/worker">
    on("change:Dead", function() {
        getAttrs(["Dead"], function(values) {
            if( values.Dead == 1) {
                var idx = 0;
                
                setAttrs({ 
               'Hungry': idx,
               'Angry': idx,
               'Afraid': idx,
               'Exhausted': idx,
               'Injured': idx,
