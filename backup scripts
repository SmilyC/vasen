<script type="text/worker">
    on("change:fresh", function() {
        getAttrs(["fresh"], function(values) {
            if( values.fresh == 1) {
                var idx = 0;
                
                setAttrs({ 
               'Hungry': idx,
               'Angry': idx,
               'Afraid': idx,
               'Exhausted': idx,
               'Injured': idx,
               'Sick': idx,
               'Dead': idx,
               'Fresh': values.fresh
               });
            }
        });
    });
</script>
<script type="text/worker">
    on("change:hungry", function() {
        getAttrs(["hungry"], function(values) {
            if( values.hungry == 1) {
                var idx = 0;
                
                setAttrs({ 
